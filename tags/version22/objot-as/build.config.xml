<?xml version="1.0" encoding="UTF-8"?>
<flex-config>
	<metadata>
		<title>Objot</title>
		<description>http://objot.com</description>
		<publisher>Qianyan Cai</publisher>
		<creator>Qianyan Cai</creator>
		<language>en</language>
	</metadata>

	<target-player>9.0.0</target-player>
	<compiler>
		<!--<locale>en_US</locale>-->

		<source-path>
			<path-element>flash</path-element>
		</source-path>
		<actionscript-file-encoding>utf-8</actionscript-file-encoding>
		<include-libraries>
			<!-- <library>lib</library> -->
		</include-libraries>

		<as3>true</as3>
		<!-- ECMAScript 3 prototype chain -->
		<es>false</es>
		<strict>true</strict>
		<optimize>true</optimize>

		<!-- false by default for mxmlc, true by default for compc -->
		<debug>true</debug>
		<!-- Show stack traces for uncaught errors -->
		<verbose-stacktraces>false</verbose-stacktraces>

		<!-- Embed, Bindable, Managed, ChangeEvent, etc -->
		<keep-as3-metadata>
			<name>Enc</name>
			<name>EncDynamic</name>
			<name>SWF</name>
			<name>Embed</name>
		</keep-as3-metadata>

		<!-- generate accessible swf -->
		<accessible>false</accessible>
		<!-- Generate generated/*.as during mxml translation -->
		<keep-generated-actionscript>false</keep-generated-actionscript>

		<!-- CSS or SWC files as Flex theme -->
		<theme>
			<!-- <filename></filename> -->
		</theme>
		<fonts>
			<!-- Provides greater clarity for small fonts -->
			<advanced-anti-aliasing>true</advanced-anti-aliasing>
			<!-- Cached embedded font faces -->
			<max-cached-fonts>20</max-cached-fonts>
			<!-- Cache characters for each font face -->
			<max-glyphs-per-face>1000</max-glyphs-per-face>
			<!-- Defines ranges that can be used across multiple font-face declarations. -->
			<!-- See flash-unicode-table.xml -->
			<languages>
				<language-range>
					<lang>All</lang>
					<range>U+0000-U+FFFF</range>
				</language-range>
				<language-range>
					<lang>Ascii</lang>
					<range>U+0020-U+007F</range>
				</language-range>
				<language-range>
					<lang>Latin</lang>
					<range>U+0020,U+0041-U+005A,U+0061-U+007A,U+00A1-U+024F,U+1E00-U+1EFF,U+2000-U+206F,U+20A0-U+20CF,U+2100-U+2183</range>
				</language-range>
				<language-range>
					<lang>Chinese</lang>
					<range>U+3000-U+303F,U+3105-U+312C,U+31A0-U+31BF,U+4E00-U+9FAF,U+FF01-U+FF60,U+F900-U+FAFF,U+201C-U+201D,U+2018-U+2019,U+2014,U+2026,U+FFE5,U+00B7</range>
				</language-range>
			</languages>
			<!-- Compiler font manager classes in policy resolution order-->
			<managers>
				<manager-class>flash.fonts.JREFontManager</manager-class>
				<manager-class>flash.fonts.AFEFontManager</manager-class>
				<manager-class>flash.fonts.BatikFontManager</manager-class>
			</managers>
			<local-fonts-snapshot></local-fonts-snapshot>
		</fonts>

		<show-actionscript-warnings>true</show-actionscript-warnings>
		<!-- Data binding warning -->
		<show-binding-warnings>true</show-binding-warnings>
		<show-deprecation-warnings>true</show-deprecation-warnings>
		<!-- toggle whether warnings generated from unused type selectors are displayed -->
		<show-unused-type-selector-warnings>true</show-unused-type-selector-warnings>

		<warn-array-tostring-changes>false</warn-array-tostring-changes>
		<warn-assignment-within-conditional>true</warn-assignment-within-conditional>
		<!-- Possibly invalid Array cast operation. -->
		<warn-bad-array-cast>true</warn-bad-array-cast>
		<!-- Non-Boolean value used where a Boolean value was expected. -->
		<warn-bad-bool-assignment>true</warn-bad-bool-assignment>
		<!-- Invalid Date cast operation. -->
		<warn-bad-date-cast>true</warn-bad-date-cast>
		<!-- Unknown method. -->
		<warn-bad-es3-type-method>true</warn-bad-es3-type-method>
		<!-- Unknown property. -->
		<warn-bad-es3-type-prop>true</warn-bad-es3-type-prop>
		<!-- Illogical comparison with NaN. note that NaN != NaN. -->
		<warn-bad-nan-comparison>true</warn-bad-nan-comparison>
		<!-- Impossible assignment to null. -->
		<warn-bad-null-assignment>true</warn-bad-null-assignment>
		<!-- Illogical comparison with null. -->
		<warn-bad-null-comparison>true</warn-bad-null-comparison>
		<!-- Illogical comparison with undefined. Only untyped or * typed variables can be undefined. -->
		<warn-bad-undefined-comparison>true</warn-bad-undefined-comparison>
		<!-- Boolean() returns false in AS3, and undefined in AS2 -->
		<warn-boolean-constructor-with-no-args>
			false
		</warn-boolean-constructor-with-no-args>
		<!-- __resolve is no longer supported. -->
		<warn-changes-in-resolve>false</warn-changes-in-resolve>
		<!-- Class is sealed.  It cannot have members added to it dynamically. -->
		<warn-class-is-sealed>true</warn-class-is-sealed>
		<!-- Constant not initialized. -->
		<warn-const-not-initialized>true</warn-const-not-initialized>
		<warn-constructor-returns-value>true</warn-constructor-returns-value>
		<warn-deprecated-event-handler-error>true</warn-deprecated-event-handler-error>
		<!-- Unsupported AS2 function. -->
		<warn-deprecated-function-error>true</warn-deprecated-function-error>
		<!-- Unsupported AS2 property. -->
		<warn-deprecated-property-error>true</warn-deprecated-property-error>
		<warn-duplicate-argument-names>true</warn-duplicate-argument-names>
		<warn-duplicate-variable-def>true</warn-duplicate-variable-def>
		<!-- in random order -->
		<warn-for-var-in-changes>false</warn-for-var-in-changes>
		<warn-import-hides-class>true</warn-import-hides-class>
		<warn-instance-of-changes>false</warn-instance-of-changes>
		<warn-internal-error>true</warn-internal-error>
		<!-- _level is no longer supported. See flash.display package. -->
		<warn-level-not-supported>true</warn-level-not-supported>
		<!-- visibility -->
		<warn-missing-namespace-decl>false</warn-missing-namespace-decl>
		<!-- Negative value will become a large positive value when assigned to a uint data type. -->
		<warn-negative-uint-literal>true</warn-negative-uint-literal>
		<warn-no-constructor>false</warn-no-constructor>
		<warn-no-explicit-super-call-in-constructor>
			false
		</warn-no-explicit-super-call-in-constructor>
		<warn-no-type-decl>false</warn-no-type-decl>
		<warn-number-from-string-changes>false</warn-number-from-string-changes>
		<!-- See bound method and function closure -->
		<warn-scoping-change-in-this>false</warn-scoping-change-in-this>
		<!-- Inefficient use of += on a TextField.-->
		<warn-slow-text-field-addition>true</warn-slow-text-field-addition>
		<!-- Possible missing parentheses. -->
		<warn-unlikely-function-value>true</warn-unlikely-function-value>
		<!-- Possible usage of the AS2 XML class. -->
		<warn-xml-class-has-changed>true</warn-xml-class-has-changed>
	</compiler>

	<!-- Runtime shared libraries will be loaded before application start -->
	<runtime-shared-libraries></runtime-shared-libraries>

	<use-network>true</use-network>

</flex-config>
